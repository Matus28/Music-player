{
  "name": "exp-next",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.ts",
    "build": "rimraf dist && tsc",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "migration:generate": "drizzle-kit generate:mysql --schema=./src/schema/*",
    "migration:push": "node -r esbuild-register src/db/migrate.ts",
    "db:push": "drizzle-kit push:mysql",
    "migration:drop": "drizzle-kit drop --config=drizzle.config.ts"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.27.2",
    "express": "^4.18.2",
    "google-auth-library": "^8.8.0",
    "googleapis": "^118.0.0",
    "jsonwebtoken": "^9.0.1",
    "music-metadata": "^7.13.0",
    "mysql2": "^3.5.1",
    "toad-scheduler": "^3.0.0",
    "ts-node": "^10.9.1",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.2",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.0",
    "drizzle-kit": "^0.19.5",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.44.0",
    "nodemon": "^2.0.22",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3"
  }
}
